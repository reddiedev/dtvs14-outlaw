{
	"exports": "./build/index.js",
	"engines": {
		"node": ">=16.11.0"
	},
	"scripts": {
		"start": "node build/index.js",
		"dev": "pnpm prebuild && pnpm build && pnpm start",
		"stop": "pnpm pm2-stop",
		"upgrade": "pnpm install",
		"serve": "pnpm prebuild && pnpm build && pnpm pm2-start",
		"restart": "pnpm run stop && pnpm run upgrade && pnpm run serve",
		"prebuild": "rimraf ./build",
		"build": "tsc --p tsconfig.json",
		"pm2-stop": "pm2 stop discord-bot",
		"pm2-start": "pm2 start build/index.js --name discord-bot --log logs.txt --time",
		"lint": "eslint --ext .js,.ts .",
		"postinstall": "prisma generate",
		"db:generate": "prisma generate",
		"db:studio": "prisma studio",
		"db:push": "prisma db push",
		"seed": "ts-node prisma/seed.ts"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.200",
		"@types/node": "^20.8.10",
		"@types/node-cron": "^3.0.8",
		"@typescript-eslint/eslint-plugin": "^6.6.0",
		"@typescript-eslint/parser": "^6.6.0",
		"eslint": "^8.48.0",
		"eslint-config-prettier": "^9.0.0",
		"prettier": "^3.0.3",
		"prisma": "^5.5.2",
		"rimraf": "^5.0.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"@prisma/client": "5.6.0",
		"chalk": "4.1.2",
		"dayjs": "^1.11.9",
		"discord.js": "^14.13.0",
		"dotenv": "^16.3.1",
		"lodash": "^4.17.21",
		"node-cache": "^5.1.2",
		"node-cron": "^3.0.2"
	},
	"prisma": {
		"seed": "ts-node prisma/seed.ts"
	}
}